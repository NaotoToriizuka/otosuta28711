<%= stylesheet_link_tag "user/new", :media => "all" %>
<%= stylesheet_link_tag "shared/header", :media => "all" %>
<%= render "shared/header" %>

<div class="contents row">
  <div class="container">
    <h2>新規登録</h2>
    <%= form_with model: @user, url: user_registration_path, id: 'new_user', class: 'new_user', local: true do |f| %>
    <%= devise_error_messages! %>
  <div class=list>
    <ul class="list-group">
      <li class="list-group-item st bg-light">ニックネーム(最大6文字)</li>
      <li class="list-group-item st bg-light">メールアドレス </li>
      <li class="list-group-item st bg-light">パスワード(英数字8文字以上)</li>
      <li class="list-group-item st bg-light">パスワード確認</li>
    </ul>
    <ul class="list-group">
      <li class="list-group-item"><%= f.text_field :nickname, autofocus: true ,class:"form" ,placeholder:"（例）OTO太郎"%></li>
      <li class="list-group-item"><%= f.email_field :email ,class: "form"%></li>
      <li class="list-group-item"><%= f.password_field :password, autocomplete: "off" ,class: "form"%></li>
      <li class="list-group-item"><%= f.password_field :password_confirmation, autocomplete: "off" ,class: "form"%></li>
    </ul>
  </div>
  <div class='actions'>
      <%= f.submit "登録", class: 'btn'%>
  </div>
    <% end %>
  </div>
</div>
